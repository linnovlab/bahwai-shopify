<div id="photo-carousel" class="absolute sm:top-[168px] top-[101px] left-0 w-full z-[-1]">
    <div class="swiper swiper-photos sm:px-8">
        <div class="items-stretch swiper-wrapper text-_main_color_dark ">
            <div class="swiper-slide !p-0">
                <div class="img">
                    <img class="w-full sm:h-[31rem] h-[29rem]"
                        src="{{ section.settings.image_1| image_url: width: 500  }}" alt="image carousel">
                </div>
            </div>
            <div class="swiper-slide !p-0">
                <div class="img">
                    <img class="w-full sm:h-[31rem] h-[29rem]"
                        src="{{ section.settings.image_2| image_url: width: 500  }}" alt="image carousel">
                </div>
            </div>
            <div class="swiper-slide !p-0">
                <div class="img">
                    <img class="w-full sm:h-[31rem] h-[29rem]"
                        src="{{ section.settings.image_3| image_url: width: 500  }}" alt="image carousel">
                </div>
            </div>
            <div class="swiper-slide !p-0">
                <div class="img">
                    <img class="w-full sm:h-[31rem] h-[29rem]"
                        src="{{ section.settings.image_4| image_url: width: 500  }}" alt="image carousel">
                </div>
            </div>
            <div class="swiper-slide !p-0">
                <div class="img">
                    <img class="w-full sm:h-[31rem] h-[29rem]"
                        src="{{ section.settings.image_5| image_url: width: 500  }}" alt="image carousel">
                </div>
            </div>
            {% comment %} <div class="swiper-slide">
                <div class="img w-[170px]">
                    <img class="w-full" src="{{ section.settings.image_1| image_url: width: 500 }}"
                        alt="image carousel">
                </div>
            </div> {% endcomment %}
        </div>
    </div>
</div>