<style>
  body{
    background-color: white !important;
  }
  .main-page-section p span{
    color: black;
    font-size: 14px;
  }
  .main-page-section p b{
    display: block;
    margin-top: 20px;
  }
</style>
<section class="engagment mt-8 mb-4">
  <div class="text-_main_color_dark my-8 mx-8">
    <h2 class="title mb-8">{{ page.title | escape }}</h2>

  </div>
  <div class="text-_main_color_dark my-8 mx-8 font-poppins">
    {{ page.content }}
  </div>

</section>



{% schema %}
  {
    "name": "t:sections.main-page.name",
    "tag": "section",
    "class": "main-page-section",
    "settings": [
      {
        "type": "header",
        "content": "t:sections.all.padding.section_padding_heading"
      }, {
        "type": "range",
        "id": "padding_top",
        "min": 0,
        "max": 100,
        "step": 4,
        "unit": "px",
        "label": "t:sections.all.padding.padding_top",
        "default": 36
      }, {
        "type": "range",
        "id": "padding_bottom",
        "min": 0,
        "max": 100,
        "step": 4,
        "unit": "px",
        "label": "t:sections.all.padding.padding_bottom",
        "default": 36
      }
    ]
  }
{% endschema %}